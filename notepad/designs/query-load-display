	query ( term )

	browse ( [ result, ... ] )

	autocomplete.select ( result )  

	( uri, graph ) = result
 
	object.setUri ( uri , graph )

		childContainer.unload().load()

		template = urilabel.discoverTemplate()

		graph += urilabel.discoverData(template)

		urilabel.update ( graph )

